{
  "name": "web-template",
  "private": true,
  "version": "1.0.0",
  "license": "MPL-2.0",
  "homepage": "https://github.com/opencomet-io/web-template",
  "repository": {
    "type": "git",
    "url": "https://github.com/opencomet-io/web-template"
  },
  "author": {
    "name": "Igor Michalak",
    "url": "https://github.com/igormichalak"
  },
  "packageManager": "yarn@3.4.1",
  "scripts": {
    "postinstall": "husky install && yarn run init-msw && yarn run init-partytown",
    "init-msw": "msw init public/ --save",
    "init-partytown": "partytown copylib public/~partytown",
    "generate-sitemaps": "next-sitemap && prettier --write './public/sitemap*.xml'",
    "build": "NODE_OPTIONS='--no-experimental-fetch' next build && yarn run postbuild",
    "postbuild": "yarn run init-partytown && yarn run generate-sitemaps",
    "dev": "NODE_OPTIONS='--no-experimental-fetch' NEXT_MANUAL_SIG_HANDLE=true next dev",
    "start": "NODE_OPTIONS='--no-experimental-fetch' NEXT_MANUAL_SIG_HANDLE=true next start --keepAliveTimeout=22000",
    "lint": "next lint",
    "format": "prettier --write .",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "coverage": "vitest run --coverage",
    "storybook": "start-storybook -p 6006 --no-open",
    "build-storybook": "build-storybook",
    "serve-storybook": "serve storybook-static"
  },
  "dependencies": {
    "@builder.io/partytown": "^0.7.5",
    "@mantine/hooks": "^5.10.3",
    "@next/env": "^13.2.3",
    "@next/font": "13.1.6",
    "downshift": "^7.2.0",
    "framer-motion": "^9.0.2",
    "i18next": "^22.4.9",
    "immer": "^9.0.19",
    "next": "13.1.6",
    "next-i18next": "^13.1.5",
    "next-pwa": "^5.6.0",
    "next-seo": "^5.15.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.1",
    "react-i18next": "^12.1.5",
    "react-is": "^18.2.0",
    "sharp": "^0.31.3",
    "styled-components": "^5.3.6",
    "tslib": "^2.5.0",
    "ua-parser-js": "^1.0.33",
    "wretch": "^2.4.1",
    "zustand": "^4.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@faker-js/faker": "^7.6.0",
    "@mdx-js/react": "^1.6.22",
    "@playwright/test": "^1.31.0",
    "@prettier/plugin-xml": "^2.2.0",
    "@storybook/addon-a11y": "^6.5.16",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-docs": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addons": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/theming": "^6.5.16",
    "@svgr/webpack": "^6.5.1",
    "@testing-library/dom": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/babel__core": "^7",
    "@types/node": "18.13.0",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/styled-components": "^5",
    "@types/ua-parser-js": "^0.7.36",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.28.4",
    "@vitest/ui": "^0.28.4",
    "babel-loader": "^8.3.0",
    "babel-plugin-styled-components": "^2.0.7",
    "dotenv": "^16.0.3",
    "eslint": "^8.34.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.6.11",
    "happy-dom": "^8.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "msw": "^1.0.1",
    "next-sitemap": "^3.1.52",
    "prettier": "^2.8.4",
    "serve": "^14.2.0",
    "storybook-addon-next": "^1.7.1",
    "svgo": "^3.0.2",
    "typescript": "4.9.5",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.28.4"
  },
  "resolutions": {
    "@types/testing-library__jest-dom": "github:opencomet-io/jest-dom-types",
    "@storybook/react-docgen-typescript-plugin": "npm:react-docgen-typescript-plugin@^1.0.5"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
